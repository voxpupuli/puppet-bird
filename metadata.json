{
  "name": "puppet-bird",
  "version": "4.1.0",
  "author": "Vox Pupuli",
  "summary": "Puppet bird Module",
  "license": "GPL-3.0",
  "source": "https://github.com/voxpupuli/puppet-bird",
  "project_page": "https://github.com/voxpupuli/puppet-bird/",
  "issues_url": "https://github.com/voxpupuli/puppet-bird/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "Archlinux"
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "9",
        "10"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "18.04",
        "20.04"
      ]
    }
  ],
  "description": "Module to install/manage Bird internet routing daemon using puppet",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.25.0 < 8.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 6.1.0 < 8.0.0"
    }
  ]
}
